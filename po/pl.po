#: gnome-keyring-ask.c:170
#, c-format
msgid "The application '%s' (%s)"
msgstr "Aplikacja \"%s\" (%s)"

#: gnome-keyring-ask.c:172
#, c-format
msgid "The application '%s'"
msgstr "Aplikacja \"%s\""

#: gnome-keyring-ask.c:175
#, c-format
msgid "The application %s"
msgstr "Aplikacja %s"

#: gnome-keyring-ask.c:185
msgid "the default keyring"
msgstr "domyślna baza kluczy"

#: gnome-keyring-ask.c:187
#, c-format
msgid "the keyring '%s'"
msgstr "baza kluczy \"%s\""

#: gnome-keyring-ask.c:190
msgid "an unknown keyring"
msgstr "nieznana baza kluczy"

#: gnome-keyring-ask.c:206
#, c-format
msgid "%s wants access to %s, but it is locked."
msgstr "%s wymaga dostępu do %s, ale baza jest zablokowana."

#: gnome-keyring-ask.c:210
msgid "Enter password for default keyring to unlock"
msgstr "Podaj hasło do domyslnej bazy kluczy aby ją odblokować"

#: gnome-keyring-ask.c:212
#, c-format
msgid "Enter password for keyring '%s' to unlock"
msgstr "Podaj hasło do bazy kluczy \"%s\" aby ją odblokować"

#: gnome-keyring-ask.c:215
msgid "Unlock Keyring"
msgstr "Odblokowanie bazy kluczy"

#: gnome-keyring-ask.c:251
#, c-format
msgid ""
"%s wants to create a new keyring called '%s'. You have to choose the "
"password you want to use for it."
msgstr ""
"%s potrzebuje utworzyć nową bazę kluczy o nazwie \"%s\". Należy podać hasło "
"jakie będzie z nią używane."

#: gnome-keyring-ask.c:255
msgid "New Keyring Password"
msgstr "Hasło nowej bazy kluczy"

#: gnome-keyring-ask.c:256
msgid "Choose password for new keyring"
msgstr "Podaj hasło dla nowej bazy kluczy"

#: gnome-keyring-ask.c:285
#, c-format
msgid ""
"%s wants to store a password, but there is no default keyring. To create "
"one, you need to choose the password you wish to use for it."
msgstr ""
"%s potrzebuje zachować hasło, ale brak domyślnej bazy kluczy. Aby ją "
"utworzyć, należy podać hasło jakie będzie z nią używane."

#: gnome-keyring-ask.c:289
msgid "Create Default Keyring"
msgstr "Tworzenie domyślnej bazy kluczy"

#: gnome-keyring-ask.c:290
msgid "Choose password for default keyring"
msgstr "Podaj hasło dla domyślnej bazy kluczy"

#: gnome-keyring-ask.c:327
msgid "Allow application access to keyring?"
msgstr "Zezwolić aplikacji na dostęp do bazy kluczy?"

#: gnome-keyring-ask.c:328
#, c-format
msgid "%s wants to access the password for '%s' in %s."
msgstr "%s wymaga hasła dostępu dla \"%s\" w %s."

#: gnome-keyring-ask.c:330
msgid "Allow access"
msgstr "Zezwolenie na dostęp"

#: gnome-keyring-ask.c:366
msgid "You must specify the type of request to run\n"
msgstr "Należy podać rodzaj żądania do uruchomienia\n"

#: gnome-keyring-ask.c:379
msgid "Unknown request type\n"
msgstr "Nieznany typ żądania\n"
